; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="GDScriptTemplate"
config/version="1.0"
run/main_scene="res://scenes/MainScene.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://_SYSTEM/HS_DesktopIcon.png"

[audio]

buses/default_bus_layout="res://assets/AudioBusLayout.tres"

[autoload]

GlobalSignals="*res://scripts/autoloads/GlobalSignals.gd"
GlobalPlaylist="*res://scripts/autoloads/global_music/GlobalPlaylist.tscn"
GlobalSettings="*res://scripts/autoloads/GlobalSettings.gd"

[file_customization]

folder_colors={
"res://addons/": "red",
"res://assets/": "orange",
"res://prefabs/": "yellow",
"res://scenes/": "green",
"res://scripts/": "teal"
}

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
